<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
     <class name="com.marklabs.tabSelector.TabConfigurator" table="tab_configurator">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
		<property name="tabName" column="tab_name"/>
		<property name="displayFlag" column="display_flag" type="short"/>
			
     </class>
</hibernate-mapping>


